---
// import CallToAction from '~/components/widgets/CallToAction.astro';
import Content from '~/components/widgets/Content.astro';
// import Features2 from '~/components/widgets/Features2.astro';
import Hero from '~/components/widgets/Hero.astro';
// import Testimonials from '~/components/widgets/Testimonials.astro';
import Layout from '~/layouts/PageLayout.astro';

const metadata = { title: 'Partners' };
---

<Layout metadata={metadata}>
  <!-- Hero Widget ******************* -->

  <Hero
    tagline="Meet our Partners"
    title="The Power of Partnerships"
    subtitle="She Leads Product couldn’t do what we do without the knowledge, wisdom, and support of our incredible partners in crime. On this page, you’ll meet the amazing collaborators who uplift, challenge, and inspire us every step of the way. </br></br><strong>Inspired to partner with us? We’d love to hear from you!</strong>"
    actions={[{ variant: 'primary', target: '_self', text: 'Contact', href: '/contact' }]}
  />

  <!-- Content Widget **************** -->

  <Content
    isReversed
    items={[
      {
        callToAction: {
          target: '_blank',
          text: 'Find out more about The Female Product Lead here',

          href: 'https://femaleproductlead.com/',
        },
        icon: 'tabler:wand',
      },
    ]}
    image={{ src: '~/assets/images/tfpl_logo_partner.png', alt: 'Features Image' }}
  >
    <Fragment slot="content">
      <h3 class="text-2xl font-bold tracking-tight dark:text-white sm:text-3xl mb-2 text-center">The Female Product Lead</h3>
      <h5 class="text-2m tracking-tight dark:text-white sm:text-3m mb-2 text-center">
        Our vision is to create Equity in Product Leadership. Everything we do, all the content we create and events we
        run are about creating a level playing field, dismantling biases, addressing systemic barriers and promoting
        inclusive opportunities that empower diverse voices and perspectives in decision-making roles which will change
        BOTH the tech world we work in and the products that are created and launched into the world.
      </h5>
      <h5 class="text-center">
        The Female Product Lead is a not for profit - Community Interest Company <br />
      </h5>
      <h5 class="text-center">Our Purpose: Empowering Female Product Leaders of Today & Tomorrow</h5>
      <h5 class="text-center">Our Promise: Free Now, Free Forever</h5>
    </Fragment>
  </Content>

